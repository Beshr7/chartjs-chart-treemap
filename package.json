{
  "name": "chartjs-chart-treemap",
  "version": "1.0.0-alpha2",
  "description": "Chart.js module for creating treemap charts",
  "main": "dist/chartjs-chart-treemap.js",
  "module": "dist/chartjs-chart-treemap.esm.js",
  "scripts": {
    "autobuild": "rollup -c -w",
    "build": "rollup -c",
    "dev": "cross-env NODE_ENV=test karma start ---auto-watch --no-single-run --browsers chrome",
    "docs": "cd docs && npm install && npm run build && mkdir -p ../dist && cp -r build ../dist/docs",
    "lint": "eslint samples/**/*.html samples/**/*.js src/**/*.js test/**/*.js",
    "test": "npm run lint && cross-env NODE_ENV=test karma start --no-auto-watch --single-run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kurkle/chartjs-chart-treemap.git"
  },
  "keywords": [
    "chart.js",
    "chart",
    "treemap"
  ],
  "files": [
    "dist/*.js"
  ],
  "author": "Jukka Kurkela",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kurkle/chartjs-chart-treemap/issues"
  },
  "homepage": "https://github.com/kurkle/chartjs-chart-treemap#readme",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-object-assign": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@rollup/plugin-babel": "^5.0.2",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "babel-plugin-istanbul": "^6.0.0",
    "chart.js": "^3.0.0-alpha",
    "codecov": "^3.7.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.1.0",
    "eslint-config-chartjs": "^0.2.0",
    "eslint-config-esnext": "^4.1.0",
    "eslint-plugin-html": "^6.0.2",
    "jasmine": "^3.5.0",
    "jasmine-core": "^3.5.0",
    "karma": "^5.0.9",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.2",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "^3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-rollup-preprocessor": "^7.0.5",
    "karma-spec-reporter": "0.0.32",
    "minimist": "^1.2.5",
    "pixelmatch": "^5.2.0",
    "rollup": "^2.12.0",
    "rollup-plugin-terser": "^6.1.0"
  },
  "peerDependencies": {
    "chart.js": "^3.0.0-alpha"
  },
  "dependencies": {}
}
